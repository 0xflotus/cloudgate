{
  "env": "PROD",
  "version": "1.0.0",
  "title": "Reverse Proxy App",
  "description": "This is a sample app using reverse proxy endpoints",
  "domains": ["*"], 
  "publicFolder": "./public",
  "TypeAPI": "LOCAL",
  "TypeFS": "LOCAL",
  "apiEndpoints": {
    "/local" : {
      "src": "./API/",
      "handler": "home.handler"
    },
    "/*" : {
      "skipPrefix": false,
      "src": "https://vms2.terasp.net/",
      "http2": false
    }
  }
}